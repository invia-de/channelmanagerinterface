{
    "name": "invia/channelmanagerinterface",
    "description": "Interface classes and objects for the Invia channel manager interface",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Torsten Burschka",
            "email": "torsten.burschka@invia.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Invia\\CMI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Invia\\Tests\\CMI\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "symfony/http-foundation": "^3.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4|^4.0"
    },
    "conflict": {
        "twig/twig": "<2.0"
    },
    "suggest": {
        "twig/twig": "Required if you want to use the TwigInterface with your adapter"
    }
}
